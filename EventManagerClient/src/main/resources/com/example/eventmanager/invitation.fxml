<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox>
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#reloadUserList" prefHeight="40.0" prefWidth="300.0" text="User List" textFill="#2200ff">
               <font>
                  <Font name="Arial Rounded MT Bold" size="20.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <VBox fx:id="containerUser" minWidth="200.0" />
         </children>
      </VBox>
   </content>
</ScrollPane>
